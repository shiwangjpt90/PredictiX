{
  "name": "predictix_backend",
  "version": "1.0.0",
  "type": "module",
  "description": "A web application that predicts if a patient has breat cancer, diabetes, heart disease and  lung cancer based on machine learning models.",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon -r dotenv/config --experimental-json-modules src/index.js",
